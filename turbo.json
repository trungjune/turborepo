{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "cache": false,
      "env": [
        ".env.development.local",
        ".env.development",
        ".env.local",
        ".env"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ],
      "env": [
        ".env.production.local",
        ".env.production",
        ".env.local",
        ".env"
      ]
    },
    "start": {
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "env": [
        ".env.test.local",
        ".env.test",
        ".env.local",
        ".env"
      ]
    }
  }
}
